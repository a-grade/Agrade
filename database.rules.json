{
	"rules": {
		".read": "false",
		".write": "false",
		"universities": {
			".read": true
		},
		"majors": {
			".read": true
		},
		"units": {
			".read": true
		},
		"users": {
			".read": "false",
			".write": "false",
			"$uid": {
				".read": "auth != null && auth.uid == $uid",
				".write": "auth != null && auth.uid == $uid"
			}
		}
	}
}
